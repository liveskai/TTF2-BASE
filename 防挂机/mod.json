{
  "Name": "laundmo.antiafk",
  "Description": "Simple Anti-AFK for servers",
  "LoadPriority": 2,
  "RequiredOnClient": false,
  "Version": "1.0.0",
  "ConVars": [
    {
      "Name": "antiafk_enabled",
      "DefaultValue": "1"
    },
    {
      "Name": "antiafk_immune",
      "DefaultValue": ""
    },
    {
      "Name": "antiafk_message",
      "DefaultValue": "警告！请不要挂机！"
    }
  ],
  "Scripts": [
    {
      "Path": "antiafk.nut",
      "RunOn": "SERVER",
      "ServerCallback": {
        "After": "antiafkInit"
      }
    }
  ]
}
